{
    "name": "loco/loco",
    "license": "MIT",
    "type": "library",
    "keywords": ["loco","translation","l10n","i18n","languages"],
    "description": "Loco SDK for PHP, including REST API client",
    "homepage" : "https://github.com/loco/loco-php-sdk",
    "authors": [
        {
            "name": "Tim Whitlock",
            "email": "support-sdk@localise.biz",
            "homepage": "https://localise.biz/",
            "role": "Developer"
        },
        {
            "name": "Alexandr Zolotukhin",
            "email": "alex@alexandrz.com",
            "homepage": "https://github.com/supersmile2009",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle-services": "^1.1",
        "symfony/console": "~2.5|~3.0|~4.0"
    },
    "require-dev" : {
        "loco/swizzle": "^2.0",
        "phpunit/phpunit": "^4.0 || ^5.0",
        "symfony/translation": "~2.5|~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
            "Loco\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Loco\\Dev\\": "dev/",
            "Loco\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
